model Lobby {
  id        String   @id @default(cuid())
  code      String   @unique
  players   Player[]
  createdAt DateTime @default(now())
}

model Player {
  id        String   @id @default(cuid())
  name      String
  lobbyId   String
  lobby     Lobby    @relation(fields: [lobbyId], references: [id])
}
